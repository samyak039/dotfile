{
  "workbench.colorTheme": "Gruvbox Material Dark",
  "editor.tabSize": 2,
  "window.menuBarVisibility": "toggle",
  "files.autoSave": "onFocusChange",
  "editor.fontFamily": "'FiraCode Nerd Font', 'Fira Code', 'monospace', monospace",
  "editor.guides.bracketPairs": "active",
  "editor.tabCompletion": "onlySnippets",
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 3,
  "editor.find.autoFindInSelection": "multiline",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "diffEditor.codeLens": true,
  "editor.minimap.renderCharacters": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "workbench.enableExperiments": false,
  "workbench.preferredDarkColorTheme": "Gruvbox Material Dark",
  "workbench.preferredLightColorTheme": "Gruvbox Material Light",
  "screencastMode.verticalOffset": 10,
  "window.autoDetectColorScheme": true,
  "window.experimental.commandCenter": true,
  "explorer.fileNesting.enabled": true,
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "foldersNestsFiles",
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "gruvboxMaterial.italicComments": false,
  "gruvboxMaterial.highContrast": true,
  "gruvboxMaterial.darkWorkbench": "flat",
  "vim.useSystemClipboard": true,
  "vim.camelCaseMotion.enable": true,
  "vim.changeWordIncludesWhitespace": true,
  "vim.cursorStylePerMode.replace": "underline",
  "vim.cursorStylePerMode.insert": "line",
  "vim.highlightedyank.color": "rgba(231, 138, 78, 0.25)",
  "vim.highlightedyank.duration": 750,
  "vim.highlightedyank.enable": true,
  "vim.leader": "",
  "vim.shell": "/bin/zsh",
  "vim.sneak": true,
  "vim.easymotion": true,
  "vim.targets.enable": true,
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "material-icon-theme",
  "editor.wordWrap": "wordWrapColumn",
  "editor.linkedEditing": true,
  "editor.lineNumbers": "relative",
  "editor.stickyScroll.enabled": true,
  "editor.formatOnPaste": true,
  "editor.minimap.size": "fill",
  "editor.acceptSuggestionOnEnter": "off",
  "workbench.tree.expandMode": "doubleClick",
  "workbench.editor.showTabs": false,
  "screencastMode.fontSize": 32,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.dialogStyle": "custom",
  "debug.confirmOnExit": "always",
  "customizeUI.font.monospace": "Fira Code",
  "customizeUI.font.regular": "Fira Sans",
  "dart.devToolsLocation": "external",
  "dart.renameFilesWithClasses": "prompt",
  "dart.flutterScreenshotPath": "~/image/screenshot/",
  "dart.runPubGetOnNestedProjects": "below",
  "dart.runPubGetOnPubspecChanges": "prompt",
  // "dart.flutterSdkPaths": ["/home/samyak/.local/share/fvm/versions/"],
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.previewFlutterUiGuides": true,
  "errorLens.fontSize": "10",
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.messageTemplate": "$severity:$code",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.mergeEditor": true,
  "git-graph.date.format": "Relative",
  "vim.foldfix": true,
  "vim.hlsearch": true,
  "vim.inccommand": "append",
  "vim.showMarksInGutter": true,
  "vim.smartRelativeLine": true,
  "gruvboxMaterial.lightWorkbench": "flat",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "git-graph.commitDetailsView.fileView.type": "File List",
  "bloc.checkForUpdates": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.renderWhitespace": "none",
  "scm.countBadge": "focused",
  "customizeUI.activityBar": "bottom",
  "git-graph.dialog.addTag.pushToRemote": true,
  "git-graph.enhancedAccessibility": true,
  "git-graph.referenceLabels.alignment": "Branches (on the left) & Tags (on the right)",
  "cSpell.userWords": [
    "Appbar",
    "ARGB",
    "autocorrect",
    "autofocus",
    "Bardiya",
    "Cumulations",
    "cupertino",
    "domlock",
    "Firestore",
    "flutterfire",
    "Fluttertoast",
    "geolocator",
    "LTRB",
    "namaste",
    "Neue",
    "Passcode",
    "Prefs",
    "pygame",
    "riverpod",
    "Samyak",
    "Showtoast",
    "signup",
    "SSID",
    "sublist",
    "subloc",
    "todos",
    "ulli",
    "unfocus",
    "vsync"
  ],
  "cSpell.showStatus": false,
  "redhat.telemetry.enabled": false,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.overviewRulerBorder": false,
  "explorer.openEditors.visible": 5,
  "files.associations": {
    "*.journal": "hledger"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "activitusbar.combineWorkspaceSettings": true,
  "activitusbar.activeColour": "#ddc7a1",
  "activitusbar.inactiveColour": "#7c6f64",
  "activitusbar.showSourceControlCounter": true,
  "activitusbar.views": [
    {
      "name": "explorer",
      "codicon": "explorer-view-icon"
    },
    {
      "name": "search",
      "codicon": "search-view-icon"
    },
    {
      "name": "scm",
      "codicon": "source-control-view-icon"
    },
    {
      "name": "debug",
      "codicon": "run-view-icon"
    },
    {
      "name": "extensions",
      "codicon": "extensions-view-icon"
    }
  ],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  }, // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.ignoreRebaseWarning": true,
  "cSpell.diagnosticLevel": "Hint",
  "workbench.activityBar.visible": false,
  "markdown.occurrencesHighlight.enabled": true,
  "markdown.preview.breaks": true,
  "markdown.updateLinksOnFileMove.enabled": "prompt",
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.print.imgToBase64": true,
  "markdown.extension.theming.decoration.renderHardLineBreak": true,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  }
}
